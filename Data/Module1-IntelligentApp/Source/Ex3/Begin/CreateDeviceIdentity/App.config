<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="hubconnectionstring" value="HostName=<IoTHub Name>.azure-devices.net;SharedAccessKeyName=iothubowner;SharedAccessKey=<IoTHub Key>" />
    <add key="docdbkey" value="<DocumentDB Key>" />
    <add key="databaseId" value="<DocumentDB Database ID>" />
    <add key="collectionId" value="<DocumentDB Collection ID>" />
    <add key="docdburi" value="https://<DocumentDB Account Name>.documents.azure.com:443/" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
